	RewriteRule ^admin/important-links-list?$ admin/important-links-list.php [NC,L]

	RewriteRule ^admin/settings/indexed-universal-life?$ admin/settings/indexed-universal-life.php [NC,L]
	RewriteRule ^admin/settings/term?$ admin/settings/term.php [NC,L]
	RewriteRule ^admin/settings/whole-life?$ admin/settings/whole-life.php [NC,L]
