Options -MultiViews

<IfModule mod_rewrite.c>

	RewriteEngine on

	RewriteRule ^admin/dashboard/?$ admin/dashboard.php [NC,L]
	RewriteRule ^advisor/dashboard/?$ advisor/dashboard.php [NC,L]

	RewriteRule ^admin/profile?$ admin/profile.php [NC,L]
	RewriteRule ^admin/profile/([^/]+)$ admin/profile.php?tab=$1 [NC,L]
	RewriteRule ^advisor/profile?$ advisor/profile.php [NC,L]
	RewriteRule ^advisor/profile/([^/]+)$ advisor/profile.php?tab=$1 [NC,L]

	RewriteRule ^admin/logout?$ admin/logout.php [NC,L]
